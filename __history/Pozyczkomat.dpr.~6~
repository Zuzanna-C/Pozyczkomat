program Pozyczkomat;

uses
  Vcl.Forms,
  LoginScreen in 'LoginScreen.pas' {Form1},
  MainScreen in 'MainScreen.pas' {Form2},
  Vcl.Themes,
  Vcl.Styles,
  DataModule in 'DataModule.pas' {DataModule1: TDataModule},
  ResetPass in 'ResetPass.pas' {Form3},
  DetailsScreen in 'DetailsScreen.pas' {Form4},
  AddEditScreen in 'AddEditScreen.pas' {Form5},
  AppSettings in 'AppSettings.pas' {Form6},
  UserSettings in 'UserSettings.pas' {Form7};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Windows10');
  Application.CreateForm(TDataModule1, DataModule1);
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm3, Form3);
  Application.CreateForm(TForm4, Form4);
  Application.CreateForm(TForm5, Form5);
  Application.CreateForm(TForm6, Form6);
  Application.CreateForm(TForm7, Form7);
  Application.Run;
end.
