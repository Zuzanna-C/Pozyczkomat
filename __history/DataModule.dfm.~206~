  object FDQueryAllTransactions: TFDQuery
    Connection = FDConnection
    SQL.Strings = (
      'SELECT'
      '  T.Id,'
      '  T.OwnerUserID,'
      '  CAST(E1.Name AS TEXT) AS OwnerName,'
      '  T.OtherUserID,'
      '  CAST(E2.Name AS TEXT) AS OtherName,'
      '  T.TypeID,'
      '  CASE '
      
        '    WHEN T.OwnerUserID = (SELECT Id FROM Entities WHERE UserId =' +
        ' :uid) '
      '      THEN '#39'po'#380'yczone komu'#347#39
      '    ELSE '#39'po'#380'yczone mnie'#39
      '  END AS TransactionRole,'
      '  CASE '
      
        '    WHEN T.OwnerUserID = (SELECT Id FROM Entities WHERE UserId =' +
        ' :uid) '
      
        '      THEN CAST(E2.Name AS TEXT)  -- je'#347'li ja jestem owner, kont' +
        'rahent = OtherUser'
      
        '    ELSE CAST(E1.Name AS TEXT)    -- je'#347'li ja jestem other, kont' +
        'rahent = Owner'
      '  END AS Counterparty,'
      '  CAST(Types.TypeName AS TEXT) AS ItemType,'
      '  T.Amount,'
      '  T.StartDate,'
      '  T.DueDate,'
      '  T.Returned,'
      '  CAST(T.ItemDescription AS TEXT) AS ItemDescription,'
      '  CAST(T.Notes AS TEXT) AS Notes,'
      '  T.CreatedByUserId'
      'FROM Transactions T'
      'JOIN Entities E1 ON T.OwnerUserID = E1.Id'
      'JOIN Entities E2 ON T.OtherUserID = E2.Id'
      'JOIN Types ON T.TypeID = Types.Id'
      
        'WHERE T.OwnerUserID = (SELECT Id FROM Entities WHERE UserId = :u' +
        'id)'
      
        '   OR T.OtherUserID = (SELECT Id FROM Entities WHERE UserId = :u' +
        'id)'
      'ORDER BY T.StartDate DESC;'
      '')
    Left = 976
    Top = 384
    ParamData = <
      item
        Name = 'UID'
        DataType = ftInteger
        ParamType = ptInput
        Value = 1
      end>
