  object FDQueryLoans: TFDQuery
    Connection = FDConnection
    SQL.Strings = (
      'SELECT '
      '  T.Id,'
      '  T.OtherUserID,            '
      '  T.TypeID,                 '
      '  '#39'lent'#39' AS Direction,'
      '  CAST(E2.Name AS TEXT) AS Counterparty,'
      '  CAST(Types.TypeName AS TEXT) AS ItemType,'
      '  T.Amount,'
      '  T.StartDate,'
      '  T.DueDate,'
      '  T.Returned,'
      '  CAST(T.ItemDescription AS TEXT) AS ItemDescription,'
      '  CAST(T.Notes AS TEXT) AS Notes,'
      '  CreatedByUserId'
      'FROM Transactions T'
      'JOIN Entities AS E2 ON T.OtherUserID = E2.Id'
      'JOIN Types ON T.TypeID = Types.Id'
      'WHERE T.OwnerUserID = ('
      '    SELECT Id FROM Entities WHERE UserId = :uid'
      ')'
      'ORDER BY T.StartDate DESC;'
      '')
    Left = 976
    Top = 24
