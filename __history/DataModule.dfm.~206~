  object FDQueryBorrowed: TFDQuery
    Connection = FDConnection
    SQL.Strings = (
      'SELECT '
      '  T.Id,'
      '  T.OwnerUserID,'
      '  T.TypeID,'
      '  '#39'borrowed'#39' AS Direction,'
      '  CAST(E1.Name AS TEXT) AS Counterparty,'
